        "emoji-mart-vue": "^2.6.3",
        "font-awesome": "^4.7.0",
        "gsap": "^3.6.0",
        "jquery": "^3.5.0",
        "laravel-echo": "^1.3.5",
        "laravel-echo-server": "^1.3.6",
        "laravel-mix": "^2.0",
        "socket.io-client": "^2.1.0",
        "vue": "^2.5.13",
        "vue-api-request": "^1.1.7",
        "vue-avatar-cropper": "^0.0.27",
